        <name>test_make_record</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[var test_make_record = Class.create();
test_make_record.prototype = {
    initialize: function() {
    },
	update:function(data) {
		gs.info('update product price called!');
		gs.info('data = ' + data);
		var product = data.product;
		var unitprice = data.unitprice;
		
		gs.info('prodcut = ' + product);
		gs.info('price = ' + unitprice);
		
		var grSales = new GlideRecord('x_211750_angular_a_sales');

		grSales.addQuery('product',product);
		grSales.query();
		
		if(grSales.next()){
		  grSales.unitprice = unitprice;
		  grSales.update(); 
		}
	},

    type: 'test_make_record'
};]]></script>
